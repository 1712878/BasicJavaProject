<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="resources.ChuDe" table="ChuDe" schema="nttuanco_Test">
        <id name="maChuDe">
            <column name="MaChuDe" sql-type="char(4)" length="4"/>
        </id>
        <property name="tenChuDe">
            <column name="TenChuDe" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <set name="deTaisByMaChuDe" inverse="true">
            <key>
                <column name="MaChuDe" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="resources.DeTai"/>
        </set>
    </class>
</hibernate-mapping>