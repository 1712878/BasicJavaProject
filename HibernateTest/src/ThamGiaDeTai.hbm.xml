<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.ThamGiaDeTai" table="ThamGiaDeTai" schema="DeTai">
        <composite-id mapped="true" class="pojo.ThamGiaDeTaiPK">
            <key-property name="maGiaoVien">
                <column name="MaGiaoVien" sql-type="varchar(10)" length="10"/>
            </key-property>
            <key-property name="maDeTai">
                <column name="MaDeTai" sql-type="varchar(10)" length="10"/>
            </key-property>
        </composite-id>
        <many-to-one name="giaoVienByMaGiaoVien" class="pojo.GiaoVien">
            <column name="MaGiaoVien"/>
        </many-to-one>
        <many-to-one name="deTaiByMaDeTai" class="pojo.DeTai">
            <column name="MaDeTai"/>
        </many-to-one>
    </class>
</hibernate-mapping>